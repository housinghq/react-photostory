{
  "name": "react-component-boilerplate",
  "description": "A boilerplate to create React components using react-transform-hmr, Babel 6, webpack and SASS",
  "main": "components/index.js",
  "scripts": {
    "lint": "eslint components/** test/**",
    "prepublish": "npm run lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "storybook": "start-storybook -p 9002",
    "test": "mocha --require test/config/setup 'tests/**/*.test.js'",
    "test:cover": "istanbul cover -x *.test.js _mocha -- -R spec --require tests/config/setup 'tests/**/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ritz078/react-component-boilerplate.git"
  },
  "keywords": [
    "react-component-keywords"
  ],
  "author": "Ritesh Kumar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritz078/react-component-boilerplate/issues"
  },
  "homepage": "https://github.com/ritz078/react-component-boilerplate",
  "devDependencies": {
    "@kadira/storybook": "^1.38.0",
    "autoprefixer": "^6.3.2",
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "commitizen": "^2.5.0",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "enzyme": "^2.4.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^2.3.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ghooks": "^1.3.2",
    "isparta": "^4.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.4.1",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "mocha": "^2.2.5",
    "node-sass": "^3.4.2",
    "rimraf": "^2.3.4",
    "sass-loader": "^3.1.2",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.0",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "@kadira/storybook-deployer": "^1.0.0",
    "classnames": "^2.2.3",
    "invariant": "^2.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
